{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/create-payment-intent",
            "methods": [
                "POST",
                "OPTIONS"
            ],
            "dest": "/index.js"
        },
        {
            "src": "/webhook",
            "methods": [
                "POST"
            ],
            "dest": "/index.js"
        },
        {
            "src": "/api/(.*)",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE",
                "OPTIONS"
            ],
            "dest": "/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ]
}